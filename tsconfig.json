{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "jsx": "react-native",
    "module": "commonjs",
    "target": "es2015",
    "baseUrl": "./",
    "paths": {
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@screens": [
        "src/screens"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@assets": [
        "src/assets"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@constants": [
        "src/constants"
      ],
      "@constants/*": [
        "src/constants/*"
      ],
      "@models": [
        "src/models"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@hooks": [
        "src/hooks"
      ],
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@store": [
        "src/store"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@context": [
        "src/context"
      ],
      "@context/*": [
        "src/context/*"
      ],
      "@mocks": [
        "src/mocks"
      ],
      "@mocks/*": [
        "src/mocks/*"
      ]
    }
  }
}
